    for(var i in sound){
      document.getElementById("demo2").innerHTML = "Sound time is: " + sound[i].time;

        var cityCircle = new google.maps.Circle({
          strokeColor: '#FF0000',
          strokeOpacity: 0.8,
          strokeWeight: 2,
          fillColor: '#FF0000',
          fillOpacity: 0.35,
          map: map,
          center: sound[i].center,
          radius: sound[i].soundLevel
        });
      
    }
  }
